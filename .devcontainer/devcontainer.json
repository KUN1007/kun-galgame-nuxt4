{
  "name": "kun-galgame-nuxt4",
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/workspaces/kun-galgame-nuxt4",
  "runServices": ["postgres", "redis", "mongo"],
  "shutdownAction": "stopCompose",
  "remoteUser": "node",
  "containerEnv": {
    "NODE_ENV": "development",
    "KUN_GALGAME_URL": "http://localhost:1007",
    "KUN_GALGAME_API": "http://localhost:1007/api",
    "KUN_DATABASE_URL": "postgresql://postgres:postgres@postgres:5432/kungalgame",
    "MONGODB_URL": "mongodb://mongo:27017/kungalgame",
    "REDIS_HOST": "redis",
    "REDIS_PORT": "6379",
    "JWT_ISS": "kun-visual-novel-dev",
    "JWT_AUD": "kun-visual-novel-dev",
    "JWT_SECRET": "dev-secret",
    "KUN_VISUAL_NOVEL_EMAIL_FROM": "kun@example.com",
    "KUN_VISUAL_NOVEL_EMAIL_HOST": "smtp.example.com",
    "KUN_VISUAL_NOVEL_EMAIL_PORT": "587",
    "KUN_VISUAL_NOVEL_EMAIL_ACCOUNT": "kun@example.com",
    "KUN_VISUAL_NOVEL_EMAIL_PASSWORD": "password",
    "KUN_VISUAL_NOVEL_IMAGE_BED_ACCESS_KEY": "dev-access-key",
    "KUN_VISUAL_NOVEL_IMAGE_BED_SECRET_KEY": "dev-secret-key",
    "KUN_VISUAL_NOVEL_IMAGE_BED_ENDPOINT": "https://image.kungal.com/kun",
    "KUN_VISUAL_NOVEL_IMAGE_BED_URL": "https://image.kungal.com",
    "KUN_VISUAL_NOVEL_IMAGE_BED_BUCKET": "image",
    "KUN_VISUAL_NOVEL_FORUM_UMAMI_ID": "dev-umami",
    "KUN_VISUAL_NOVEL_FORUM_YANDEX_VERIFICATION": "dev-yandex",
    "KUN_CF_CACHE_ZONE_ID": "dev-zone",
    "KUN_CF_CACHE_PURGE_API_TOKEN": "dev-cf-token",
    "KUN_VISUAL_NOVEL_S3_STORAGE_ACCESS_KEY_ID": "dev-s3-key",
    "KUN_VISUAL_NOVEL_S3_STORAGE_SECRET_ACCESS_KEY": "dev-s3-secret",
    "KUN_VISUAL_NOVEL_S3_STORAGE_BUCKET_NAME": "kun-love-ren",
    "KUN_VISUAL_NOVEL_S3_STORAGE_ENDPOINT": "https://s3.dev.local",
    "KUN_VISUAL_NOVEL_S3_STORAGE_REGION": "us-west-001",
    "KUN_VISUAL_NOVEL_S3_STORAGE_URL": "https://kun-s3.dev"
  },
  "forwardPorts": [1007, 24678, 5432, 6379, 27017],
  "portsAttributes": {
    "1007": {
      "label": "Nuxt Dev Server"
    },
    "24678": {
      "label": "Vite HMR"
    },
    "5432": {
      "label": "PostgreSQL"
    },
    "6379": {
      "label": "Redis"
    },
    "27017": {
      "label": "MongoDB"
    }
  },
  "onCreateCommand": "if [ ! -f .env ]; then cp .env.example .env; fi",
  "postCreateCommand": "pnpm install && pnpm prisma:generate",
  "customizations": {
    "vscode": {
      "extensions": [
        "Vue.volar",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Prisma.prisma",
        "ms-azuretools.vscode-docker",
        "eamodio.gitlens"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "files.eol": "\n",
        "editor.codeActionsOnSave": {
          "source.fixAll.eslint": "explicit"
        }
      }
    }
  }
}
